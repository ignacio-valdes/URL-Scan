{
    "manifest_version":3,
    "name": "URL-Scan",
    "description": "A simple URL scanner extension.",
    "version": "1.0",
    "icons": {
        "128": "icons/logo.png"
    },
    "action":{ 
        "default_icon": "icons/logo.png",
        "default_title": "URL-Scan",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}